getgenv().autoScents = false
--GUI
local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local Window = Library:NewWindow("Artifact script")

local Tab1 = Window:NewSection("Main")
local Tab2 = Window:NewSection("Warden Jewel")
----

--Functions
function teleportTo(placeCFrame)
	local plyr = game.Players.LocalPlayer
	if plyr.Character then
		plyr.Character.HumanoidRootPart.CFrame = placeCFrame
	end
end

function autoScentHidden()
    while getgenv().autoScents == true do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("HideScent"):FireServer()
        wait(1)
    end
end
----

--Script

local scentLocal
Tab1:CreateToggle("Hide scents", function(hideScents)
    local scentLocal = hideScents
    if scentLocal == true then
        getgenv().autoScents = true     
        autoScentHidden()
    elseif scentLocal == false then
        getgenv().autoScents = false
    end
end)

Tab1:CreateButton("Safe place (load flower cove)", function()
    teleportTo(workspace.Terrain["Warden Shrines"].Verdant:GetChildren()[5].CFrame)
end)

local eigionLocal = game:GetService("Players").LocalPlayer.PlayerGui.Data.WardenShrines.ShrineValues.Eigion.Value
local eigionDropdown
Tab2:CreateDropdown("Eigion", {"Sacrifice food", "Progression/1000 :" .. eigionLocal, "Eigion"},  3, function(itemCreatureEigion)
    eigionDropdown = itemCreatureEigion
    if eigionDropdown == "Sacrifice food" then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Syphix1919/Creatures-of-sonaria-keido-ui-beta-wip/main/wip%20script%20help%201%20(eigion)"))()
    elseif eigionDropdown == "Progression" then
        Tab2:CreateButton("Button", function()
            print("HI")
        end)
    end
end)




