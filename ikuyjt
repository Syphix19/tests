			GUI:Notification{
				Title = "Verdant progression before executing :",
				Text = verdantLocalText,
				Duration = 3,
				Callback = function() end
			}
			wait(0.5)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Syphix1919/Creatures-of-sonaria-keido-ui-beta-wip/main/wip%20script%20help%206%20(verdant)"))()
		elseif verdantDropdown == 2 then
			local verdantLocal = game:GetService("Players").LocalPlayer.PlayerGui.Data.WardenShrines.ShrineValues.Verdant.Value
			local verdantLocalText = verdantLocal .. "/500"
			GUI:Prompt{
				Followup = false,
				Title = "Progress",
				Text = verdantLocalText,
				Buttons = {
					ok = function()
						return true
					end,
				}
			}
		end	
	return end
}

local borealDropdown
Tab6:Dropdown{
	Name = "Boreal",
	StartingText = "Select...",
	Description = nil,
	Items = {
		{"Sacrifice food", 1},
		{"Progression", 2}
	},
	Callback = function(itemCreatureBoreal) 
		borealDropdown = itemCreatureBoreal
		if borealDropdown == 1 then
			local borealLocal = game:GetService("Players").LocalPlayer.PlayerGui.Data.WardenShrines.ShrineValues.Boreal.Value
			local borealLocalText = borealLocal .. "/750"
			GUI:Notification{
				Title = "Boreal progression before executing :",
				Text = borealLocalText,
				Duration = 3,
				Callback = function() end
			}
			wait(0.5)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Syphix1919/Creatures-of-sonaria-keido-ui-beta-wip/main/wip%20script%20help%205%20(boreal)"))()
		elseif borealDropdown == 2 then
			local borealLocal = game:GetService("Players").LocalPlayer.PlayerGui.Data.WardenShrines.ShrineValues.Boreal.Value
			local borealLocalText = borealLocal .. "/750"
			GUI:Prompt{
				Followup = false,
				Title = "Progress",
				Text = borealLocalText,
				Buttons = {
					ok = function()
						return true
					end,
				}
			}
		end	
	return end
}

local whatSlot
Tab7:Dropdown{
	Name = "What slot is your creature with injury ?",
	StartingText = "Select...",
	Description = nil,
	Items = {
		{Slot1Name .. ": Slot 1", "Slot1"},
		{Slot2Name .. ": Slot 2", "Slot2"},
		{Slot3Name .. ": Slot 3", "Slot3"},
		{Slot4Name .. ": Slot 4", "Slot4"},
		{Slot5Name .. ": Slot 5", "Slot5"},
		{Slot6Name .. ": Slot 6", "Slot6"},
		{Slot7Name .. ": Slot 7", "Slot7"},
		{Slot8Name .. ": Slot 8", "Slot8"},
		{Slot9Name .. ": Slot 9", "Slot9"},
		{Slot10Name .. ": Slot 10", "Slot10"},
		{Slot11Name .. ": Slot 11", "Slot11"},
		{Slot12Name .. ": Slot 12", "Slot12"},
		{Slot13Name .. ": Slot 13", "Slot13"}
	},
	Callback = function(whatSlotDropdown) 
		whatSlot = whatSlotDropdown
	return end
}

Tab7:Button{
	Name = "Spawn as selected (recommended to play manually)",
	Description = nil,
	Callback = function() 
		local args = {
			[1] = whatSlot
		}
		
		game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SpawnRemote"):InvokeServer(unpack(args))
	end
}

Tab7:Button{
	Name = "Despawn creature",
	Description = nil,
	Callback = function() 
		game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DespawnRemote"):InvokeServer()
	end
}

Tab7:Button{
	Name = "Get injury (lmakosauruodon recommended)",
	Description = nil,
	Callback = function() 
		loadstring(game:HttpGet("https://github.com/Syphix19/Creatures-of-sonaria-syphix-keido-ui-main/raw/main/Injury"))()
	end
}

local localInjuryState
Tab7:Toggle{
	Name = "Auto get injury",
	StartingState = false,
	Description = nil,
	Callback = function(Injurystate)
		localInjuryState = Injurystate
		if localInjuryState == true then
			getgenv().autoGetInjury = true
			autoInjury()
		elseif localInjuryState == false then
			getgenv().autoGetInjury = false
		end
	end
}


