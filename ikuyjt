Tab5:Textbox({
	Name = "Create slots/spam slots (input full creature name)",
	PlaceholderText = "",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text1)
		local args = {
			[1] = (Text1)
		}
		
		game:GetService("ReplicatedStorage").Remotes.CreateSlotRemote:InvokeServer(unpack(args))
		wait()
		local args = {
			[1] = (Text1)
		}
		
		game:GetService("ReplicatedStorage").Remotes.CreateSlotRemote:InvokeServer(unpack(args))
		wait()
		local args = {
			[1] = (Text1)
		}
		
		game:GetService("ReplicatedStorage").Remotes.CreateSlotRemote:InvokeServer(unpack(args))
		wait()
	end,
 })

