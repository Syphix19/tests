--Functions
function getPlayerPos()
	local plyr = game.Players.LocalPlayer
	if plyr.Character then
		return plyr.Character.HumanoidRootPart.Position
	end
		return false
end

function teleportTo(placeCFrame)
	local plyr = game.Players.LocalPlayer
	if plyr.Character then
		plyr.Character.HumanoidRootPart.CFrame = placeCFrame
	end
end

function autoDrink()
	while getgenv().alwaysDrink == true do
		local args = {
			[1] = workspace.Interactions.Lakes.Lake
		}
		game:GetService("ReplicatedStorage").Remotes.DrinkRemote:FireServer(unpack(args))
		wait(0.5)
	end
end

function autoSniff()
	getgenv().alwaysSniff = true
    while getgenv().alwaysSniff == true do
        local args = {
            [1] = 1
        }
        
        game:GetService("ReplicatedStorage").Remotes.SetMissionRemote:FireServer(unpack(args))
        wait(1)        
    end
end

function autoInjury()
	while getgenv().autoGetInjury == true do
		loadstring(game:HttpGet("https://github.com/Syphix19/Creatures-of-sonaria-syphix-keido-ui-main/raw/main/Injury"))()
		wait(30)
	end
end

--- 
local localSelectedCreature
Tab0:Dropdown{
	Name = "Select creature",
	StartingText = "Select...",
	Description = nil,
	Items = {
		{Slot1Name .. ": Slot 1", "Slot1"},
		{Slot2Name .. ": Slot 2", "Slot2"},
		{Slot3Name .. ": Slot 3", "Slot3"},
		{Slot4Name .. ": Slot 4", "Slot4"},
		{Slot5Name .. ": Slot 5", "Slot5"},
		{Slot6Name .. ": Slot 6", "Slot6"},
		{Slot7Name .. ": Slot 7", "Slot7"},
		{Slot8Name .. ": Slot 8", "Slot8"},
		{Slot9Name .. ": Slot 9", "Slot9"},
		{Slot10Name .. ": Slot 10", "Slot10"},
		{Slot11Name .. ": Slot 11", "Slot11"},
		{Slot12Name .. ": Slot 12", "Slot12"},
		{Slot13Name .. ": Slot 13", "Slot13"}
	},
	Callback = function(itemselectedCreature) 
		localSelectedCreature = itemselectedCreature
		if localSelectedCreature == "Slot1" then
			localSelectedCreature = Slot1Gender
		elseif localSelectedCreature == "Slot2" then
			localSelectedCreature = Slot2Gender
		elseif localSelectedCreature == "Slot3" then
			localSelectedCreature = Slot3Gender
		elseif localSelectedCreature == "Slot4" then
			localSelectedCreature = Slot4Gender
		elseif localSelectedCreature == "Slot5" then
			localSelectedCreature = Slot5Gender
		elseif localSelectedCreature == "Slot6" then
			localSelectedCreature = Slot6Gender
		elseif localSelectedCreature == "Slot7" then
			localSelectedCreature = Slot7Gender
		elseif localSelectedCreature == "Slot8" then
			localSelectedCreature = Slot8Gender
		elseif localSelectedCreature == "Slot9" then
			localSelectedCreature = Slot9Gender
		elseif localSelectedCreature == "Slot10" then
			localSelectedCreature = Slot10Gender
		elseif localSelectedCreature == "Slot11" then
			localSelectedCreature = Slot11Gender
		elseif localSelectedCreature == "Slot12" then
			localSelectedCreature = Slot12Gender
		elseif localSelectedCreature == "Slot3" then
			localSelectedCreature = Slot13Gender
		end
	return end
}

local localCreatureGender
Tab0:Dropdown{
	Name = "Creature gender",
	StartingText = "Select...",
	Description = nil,
	Items = {
		{"Set to male", "Male"},
		{"Set to female", "Female"},
		{"Get current gender", "Current gender"}
	},
	Callback = function(itemCreatureGender) 
		localCreatureGender = itemCreatureGender
		if localCreatureGender == "Male" then
			localSelectedCreature = true
		elseif localCreatureGender == "Female" then
			localSelectedCreature = false
		end
	return end
}

Tab1:Textbox({
   Name = "What do you want to send ?",
   PlaceholderText = "",
   RemoveTextAfterFocusLost = false,
   Callback = function(Text)
   getgenv().whatToSay = true
   while getgenv().whatToSay == true do
	   local args = {
    		[1] = (Text), --the message you wanna say (between " and the other ")
    		[2] = "All"
    		}
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			wait(5)
   end
   end,
})


Tab1:Button({
   Name = "Stop talking",
   Callback = function()
   getgenv().whatToSay = false
   end,
})


