        Others = Window:AddTab({ Title = "Others", Icon = "settings" }),
    }
    local Tabs = {
        Main = Window:AddTab({ Title = "Main", Icon = "home" }),
        Others = Window:AddTab({ Title = "Others", Icon = "settings" }),
    }

