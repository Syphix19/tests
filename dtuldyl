local whatSlot
Tab4:Dropdown{
	Name = "What slot is your creature with injury ?",
	StartingText = "Select...",
	Description = nil,
	Items = {
		{Slot1Name .. ": Slot 1", "Slot1"},
		{Slot2Name .. ": Slot 2", "Slot2"},
		{Slot3Name .. ": Slot 3", "Slot3"},
		{Slot4Name .. ": Slot 4", "Slot4"},
		{Slot5Name .. ": Slot 5", "Slot5"},
		{Slot6Name .. ": Slot 6", "Slot6"},
		{Slot7Name .. ": Slot 7", "Slot7"},
		{Slot8Name .. ": Slot 8", "Slot8"},
		{Slot9Name .. ": Slot 9", "Slot9"},
		{Slot10Name .. ": Slot 10", "Slot10"},
		{Slot11Name .. ": Slot 11", "Slot11"},
		{Slot12Name .. ": Slot 12", "Slot12"},
		{Slot13Name .. ": Slot 13", "Slot13"}
	},
	Callback = function(whatSlotDropdown) 
		whatSlot = whatSlotDropdown
	return end
}

Tab4:Button{
	Name = "Spawn as selected (recommended to play manually)",
	Description = nil,
	Callback = function() 
		local args = {
			[1] = whatSlot
		}
		
		game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SpawnRemote"):InvokeServer(unpack(args))
	end
}

Tab4:Button{
	Name = "Despawn creature",
	Description = nil,
	Callback = function() 
		game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DespawnRemote"):InvokeServer()
	end
}

Tab4:Button{
	Name = "Get injury (lmakosauruodon recommended)",
	Description = nil,
	Callback = function() 
		loadstring(game:HttpGet("https://github.com/Syphix19/Creatures-of-sonaria-syphix-keido-ui-main/raw/main/Injury"))()
	end
}

local localInjuryState
Tab4:Toggle{
	Name = "Auto get injury",
	StartingState = false,
	Description = nil,
	Callback = function(Injurystate)
		localInjuryState = Injurystate
		if localInjuryState == true then
			getgenv().autoGetInjury = true
			autoInjury()
		elseif localInjuryState == false then
			getgenv().autoGetInjury = false
		end
	end
}



Tab5:Button({
	Name = "Insta mud (must stand in mud before clicking)(keybind below)",
   Callback = function()
	local args = {
		[1] = workspace.Interactions.Mud
	}
	 
	game:GetService("ReplicatedStorage").Remotes.Mud:FireServer(unpack(args))
   end,
})

Tab5:keybind({Callback = function()
	local args = {
		[1] = workspace.Interactions.Mud
	}
	 
	game:GetService("ReplicatedStorage").Remotes.Mud:FireServer(unpack(args))
end,})

Tab5:Textbox({
	Name = "Create slots/spam slots (input full creature name)",
	PlaceholderText = "",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text1)
		local args = {
			[1] = (Text1)
		}
		
		game:GetService("ReplicatedStorage").Remotes.CreateSlotRemote:InvokeServer(unpack(args))
		wait()
		local args = {
			[1] = (Text1)
		}
		
		game:GetService("ReplicatedStorage").Remotes.CreateSlotRemote:InvokeServer(unpack(args))
		wait()
		local args = {
			[1] = (Text1)
		}
		
		game:GetService("ReplicatedStorage").Remotes.CreateSlotRemote:InvokeServer(unpack(args))
		wait()
	end,
 })

 Tab5:Button{
	Name = "Get 13 storage slot",
	Description = "Resets when you leave EXEPT for the creatures you stored",
	Callback = function() 
		game.Players.LocalPlayer.PlayerGui.Data.StorageCap.Value = 13
	end
}

Tab5:Button{
	Name = "Take damage",
	Description = "Attack yourself to lose health",
	Callback = function() 
		loadstring(game:HttpGet("https://github.com/Syphix19/Creatures-of-sonaria-syphix-keido-ui-main/raw/main/Injury"))()
	end
}

Tab6:Textbox({
	Name = "What do you want to send ?",
	PlaceholderText = "",
	RemoveTextAfterFocusLost = false,
	Callback = function(Text)
	getgenv().whatToSay = true
	while getgenv().whatToSay == true do
		local args = {
			 [1] = (Text),
			 [2] = "All"
			 }
			 game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			 wait(5)
	end
	end,
 })
 
 
 Tab6:Button({
	Name = "Stop talking",
	Callback = function()
	getgenv().whatToSay = false
	end,
 })

