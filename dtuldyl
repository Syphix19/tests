--Functions
function getPlayerPos()
	local plyr = game.Players.LocalPlayer
	if plyr.Character then
		return plyr.Character.HumanoidRootPart.Position
	end
		return false
end

function teleportTo(placeCFrame)
	local plyr = game.Players.LocalPlayer
	if plyr.Character then
		plyr.Character.HumanoidRootPart.CFrame = placeCFrame
	end
end

function autoDrink()
	while getgenv().alwaysDrink == true do
		local args = {
			[1] = workspace.Interactions.Lakes.Lake
		}
		game:GetService("ReplicatedStorage").Remotes.DrinkRemote:FireServer(unpack(args))
		wait(0.5)
	end
end

function autoSniff()
	getgenv().alwaysSniff = true
    while getgenv().alwaysSniff == true do
        local args = {
            [1] = 1
        }
        
        game:GetService("ReplicatedStorage").Remotes.SetMissionRemote:FireServer(unpack(args))
        wait(1)        
    end
end

function autoInjury()
	while getgenv().autoGetInjury == true do
		loadstring(game:HttpGet("https://github.com/Syphix19/Creatures-of-sonaria-syphix-keido-ui-main/raw/main/Injury"))()
		wait(30)
	end
end

function loopDestroyChocolate()
	while getgenv().autoDestroyChocolatePiles == true do
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Syphix19/Creatures-of-sonaria-syphix-keido-ui-main/main/Protected%20main%20help%202", true))()
		wait(1)
	end
end

function loopGetMails()
	while getgenv().autoGetMails == true do
		local args = {
			[1] = workspace:WaitForChild("Valentine Event"):WaitForChild("VolcanoMail")
		}
		
		game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GetLetterRemote"):InvokeServer(unpack(args))
		wait()
		local args = {
			[1] = workspace:WaitForChild("Valentine Event"):WaitForChild("PrideMail")
		}
		
		game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GetLetterRemote"):InvokeServer(unpack(args))
		wait(1)
	end
end

function loopDeliverMails()
	while getgenv().autoDeliverMailsGenv == true do
		local args = {
			[1] = workspace:WaitForChild("Valentine Event"):WaitForChild("Mailbox"):WaitForChild("Mailbox")
		}
		
		game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DeliverLetterRemote"):FireServer(unpack(args))
		wait(5)
	end
end

--- 
EventTab:Button{
	Name = "Destroy chocolate piles",
	Description = nil,
	Callback = function() 
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Syphix19/Creatures-of-sonaria-syphix-keido-ui-main/main/Protected%20main%20help%202", true))()
	end
}

local localMailCollect
EventTab:Dropdown{
	Name = "Select a mail to collect",
	StartingText = "Select...",
	Description = nil,
	Items = {
		{"Pride rocks mail", 1},
		{"Volcano mail", 2},
		{"Both", 3}
	},
	Callback = function(functionMail) 
		localMailCollect = functionMail
		if functionMail == 1 then
			local args = {
				[1] = workspace:WaitForChild("Valentine Event"):WaitForChild("PrideMail")
			}
			
			game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GetLetterRemote"):InvokeServer(unpack(args))
		elseif functionMail == 2 then
			local args = {
				[1] = workspace:WaitForChild("Valentine Event"):WaitForChild("VolcanoMail")
			}
			
			game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GetLetterRemote"):InvokeServer(unpack(args))
		elseif functionMail == 3 then
			local args = {
				[1] = workspace:WaitForChild("Valentine Event"):WaitForChild("PrideMail")
			}
			
			game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GetLetterRemote"):InvokeServer(unpack(args))
			wait()
			local args = {
				[1] = workspace:WaitForChild("Valentine Event"):WaitForChild("VolcanoMail")
			}
			
			game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GetLetterRemote"):InvokeServer(unpack(args))
		end
	return end
}

EventTab:Button({
    Name = "Deliver mail",
    Callback = function()
		local args = {
			[1] = workspace:WaitForChild("Valentine Event"):WaitForChild("Mailbox"):WaitForChild("Mailbox")
		}
		
		game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DeliverLetterRemote"):FireServer(unpack(args))
    end,
 })

 EventTab:Button({
    Name = "Mail deliver cooldown",
    Callback = function()
		GUI:Prompt{
			Followup = false,
			Title = "Cooldown : ",
			Text = game:GetService("Workspace").ValentineEvent.Mailbox.Mailbox.TimerGui.TimerLabel.Text,
			Buttons = {
				ok = function()
					return true
				end,
			}
		}
    end,
 })



local autoDestroyChocolate
EventTab:Toggle{
	Name = "Auto destroy chocolate piles",
	StartingState = false,
	Description = nil,
	Callback = function(autoChocolateState) 
		autoDestroyChocolate = autoChocolateState
		if autoDestroyChocolate == true then 
			getgenv().autoDestroyChocolatePiles = true
			loopDestroyChocolate()
		elseif autoDestroyChocolate == false then
			getgenv().autoDestroyChocolatePiles = false
			loopDestroyChocolate()
		end
	end
}

local autoCollectMail
EventTab:Toggle{
	Name = "Auto collect mails",
	StartingState = false,
	Description = nil,
	Callback = function(autoMailState) 
		autoCollectMail = autoMailState
		if autoCollectMail == true then 
			getgenv().autoGetMails = true
			loopGetMails()
		elseif autoCollectMail == false then
			getgenv().autoGetMails = false
		end
	end
}

local autoDeliverMail
EventTab:Toggle{
	Name = "Auto Deliver mails",
	StartingState = false,
	Description = nil,
	Callback = function(autoMailDeliverState) 
		autoDeliverMail = autoMailDeliverState
		if autoDeliverMail == true then 
			getgenv().autoDeliverMailsGenv = true
			loopDeliverMails()
		elseif autoDeliverMail == false then
			getgenv().autoDeliverMailsGenv = false
		end
	end
}

