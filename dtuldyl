getgenv().whatToSay = false
getgenv().alwaysDrink = false
getgenv().alwaysSniff = false
getgenv().autoGetInjury = false
getgenv().autoDestroyChocolatePiles = false
getgenv().autoGetMails = false
getgenv().autoDeliverMailsGenv = false

