	Name = "Recode : enter your key",
	PlaceholderText = "",
	RemoveTextAfterFocusLost = false,
	Callback = function(TextPremium1)
		getgenv().Key = (TextPremium1)
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Mangnex/Lycan-X-Hub/main/Cos_Premium"))() 
	end,
 })



